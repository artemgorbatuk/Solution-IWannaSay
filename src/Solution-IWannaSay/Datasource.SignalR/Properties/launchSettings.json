{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  {
    "profiles": {
      "LocalLaunch": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "applicationUrl": "http://localhost:5000;https://localhost:5001",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "DockerLaunch": {
        "commandName": "Docker",
        "launchBrowser": true,
        "httpPort": 5000,
        "useSSL": false
      }
    }
  }
}