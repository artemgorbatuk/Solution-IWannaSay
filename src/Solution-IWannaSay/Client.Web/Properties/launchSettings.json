{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "MvcApp": {
      "commandName": "Project",
      "launchBrowser": true,
      "httpPort": 5000,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTP_PORTS": "6000"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "httpPort": 5000,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "ASPNETCORE_HTTP_PORTS": "6000"
      },
      "applicationUrl": "http://localhost:5000"
    }
  }
}